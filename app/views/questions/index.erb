<div>
	<h1>Welcome to Hacker Stacker</h1>
	<h2>Select a question to explore, or login to post a question or comment</h2>
	<ul>
	<% @questions.each do |question| %>
		<li>
			<a href="/questions/<%= question.id %>"><%= question.title %></a><br>
			<p>Question:<%= question.text %></p>
			<p>Votes:<%= question.votes.count %></p>
				<% question.answers.each do |answer| %>
				<p>Answer:<%= answer.text %></p>
				<% end %>
		</li>
		<% end %>
	</ul>

</div>
